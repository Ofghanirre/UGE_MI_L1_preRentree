<!DOCTYPE html>
<html$if(lang)$ lang="$lang$"$endif$>
<head>
	<meta charset="utf-8">
  	<meta name="generator" content="pandoc">
  	<meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=yes">
$for(author-meta)$
	<meta name="author" content="$author-meta$">
$endfor$
$if(date-meta)$
    <meta name="dcterms.date" content="$date-meta$">
$endif$
    <title>$if(title-prefix)$$title-prefix$ - $endif$$pagetitle$</title>
    <link rel="shortcut icon" href="https://elearning.u-pem.fr/theme/image.php/bootsup/theme/1442579361/favicon" />
    <link rel="stylesheet" type="text/css" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
    <link rel='stylesheet' type='text/css' href='//fonts.googleapis.com/css?family=Droid+Sans:400,700'>
	<link rel="stylesheet" type="text/css" href="https://elearning.u-pem.fr/theme/styles.php/bootsup/1442579361/all" />
    <style type="text/css">code{white-space: pre;}</style>
    <style type="text/css">body{margin:10px}</style>
    <!--[if lt IE 9]>
      <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
$if(quotes)$
    <style type="text/css">q { quotes: "“" "”" "‘" "’"; }</style>
$endif$
$if(highlighting-css)$
    <style type="text/css">
$highlighting-css$
    </style>
$endif$
$for(css)$
    <link rel="stylesheet" href="$css$">
$endfor$
$if(math)$
    $math$
$endif$
$for(header-includes)$
    $header-includes$
$endfor$
</head>

<body>

$for(include-before)$
$include-before$
$endfor$

$if(headers)$
$if(title)$
<header>
<h1 class="title">$title$</h1>
$if(subtitle)$
<h1 class="subtitle">$subtitle$</h1>
$endif$
$for(author)$
<h2 class="author">$author$</h2>
$endfor$
$if(date)$
<h3 class="date">$date$</h3>
$endif$
</header>
$endif$
$if(toc)$
<nav id="$idprefix$TOC">
$toc$
</nav>
$endif$
$endif$

$body$

$for(include-after)$
$include-after$
$endfor$
</body>
</html>
